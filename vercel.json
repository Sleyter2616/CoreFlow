{
  "version": 2,
  "buildCommand": "prisma generate && prisma db push --accept-data-loss && npm run seed:prod && next build",
  "installCommand": "npm install",
  "framework": "nextjs",
  "regions": ["iad1"],
  "env": {
    "NEXTAUTH_URL": "https://${VERCEL_URL}",
    "VERCEL_POSTGRES_PRISMA_URL": "${POSTGRES_PRISMA_URL}",
    "VERCEL_POSTGRES_URL_NON_POOLING": "${POSTGRES_URL_NON_POOLING}"
  }
}
